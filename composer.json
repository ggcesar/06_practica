{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "andreaselia/laravel-api-to-postman": "^1.15",
        "barryvdh/laravel-dompdf": "^3.1",
        "darkaonline/l5-swagger": "^8.4",
        "diglactic/laravel-breadcrumbs": "^9.0",
        "doctrine/dbal": "^3.3",
        "guzzlehttp/guzzle": "^7.2",
        "halilcosdu/laravel-slower": "^2.0",
        "hammerstone/fast-paginate": "^0.1.12",
        "jenky/hades": "^2.0",
        "laravel/framework": "10.48.22",
        "laravel/passport": "^12.0",
        "laravel/tinker": "^2.7",
        "laravel/ui": "^4.0",
        "laravelcollective/html": "^6.3",
        "laravolt/avatar": "^5.0",
        "livewire/livewire": "^2.10",
        "maatwebsite/excel": "^3.1",
        "predis/predis": "^2.0",
        "pusher/pusher-php-server": "^7.2",
        "rap2hpoutre/laravel-log-viewer": "^2.2",
        "rappasoft/laravel-authentication-log": "^3.0",
        "spatie/laravel-medialibrary": "^10.3",
        "spatie/laravel-permission": "^5.5",
        "torann/geoip": "^3.0",
        "vinkla/hashids": "^11.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.6",
        "enlightn/enlightn": "^2.10",
        "fakerphp/faker": "^1.9.1",
        "laravel-lang/lang": "^10.9",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "mtolhuys/laravel-schematics": "dev-master",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.0",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan vendor:publish --force --tag=livewire:assets --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true,
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
